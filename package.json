{
  "name": "gotobyu.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "concurrently -n eslint,stylelint,prettier -r 'eslint .' 'stylelint .' 'prettier --check --loglevel warn .'",
    "lint:fix": "concurrently -n eslint,stylelint,prettier -r 'eslint --fix .' 'stylelint --fix .' 'prettier --write --loglevel warn .'",
    "ver": "node ./src/bin/get-version",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o .storybook-static",
    "algolia": "node --loader ./src/bin/loader.mjs --experimental-specifier-resolution=node ./src/bin/algolia.ts",
    "ut": "node --experimental-vm-modules --experimental-json-modules --experimental-specifier-resolution=node $(yarn bin jest)",
    "ut:debug": "node --experimental-vm-modules --experimental-json-modules --experimental-specifier-resolution=node --inspect $(yarn bin jest) --runInBand",
    "e2e": "cross-env NODE_ENV=test playwright test",
    "e2e:linux": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v$(yarn --silent ver @playwright/test)-focal /bin/bash -c 'yarn e2e --retries=2'",
    "e2e:linux:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v$(yarn --silent ver @playwright/test)-focal /bin/bash -c 'yarn e2e --update-snapshots'"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.3",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/loader": "^2.1.1",
    "@next/mdx": "^12.1.6",
    "algoliasearch": "^4.13.1",
    "axios": "^0.27.2",
    "chart.js": "^3.8.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "classnames": "^2.3.1",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "react": "^18.1.0",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.1.0",
    "react-instantsearch-dom": "^6.26.0",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.2",
    "rehype-parse": "7.x",
    "rehype-slug": "4.x",
    "remark-sectionize": "^1.1.1",
    "sharp": "^0.30.5",
    "slick-carousel": "^1.8.1",
    "unified": "9.x",
    "unist-util-visit": "2.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@next/env": "^12.1.6",
    "@next/eslint-plugin-next": "^12.1.6",
    "@playwright/test": "^1.22.2",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-react": "^1.2.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
