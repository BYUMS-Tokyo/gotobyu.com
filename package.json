{
  "name": "gotobyu.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "concurrently -n eslint,stylelint,prettier -r 'eslint .' 'stylelint .' 'prettier --check --loglevel warn .'",
    "lint:fix": "concurrently -n eslint,stylelint,prettier -r 'eslint --fix .' 'stylelint --fix .' 'prettier --write --loglevel warn .'",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o .storybook-static",
    "algolia": "node --loader ./src/bin/loader.mjs --experimental-specifier-resolution=node ./src/bin/algolia.ts",
    "ut": "node --experimental-vm-modules --experimental-json-modules $(yarn bin jest)",
    "ut:debug": "node --experimental-vm-modules --experimental-json-modules --inspect $(yarn bin jest) --runInBand",
    "e2e": "playwright test",
    "e2e:linux": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.21.1-focal /bin/bash -c 'yarn e2e --retries=2'",
    "e2e:linux:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.21.1-focal /bin/bash -c 'yarn e2e --update-snapshots'"
  },
