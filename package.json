    "storybook-addon-next-router": "^4.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.5",
