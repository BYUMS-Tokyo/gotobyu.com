  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "export": "UNOPTIMIZE_IMAGE=true next export",
    "start": "next start",
    "lint": "concurrently -r 'eslint .' 'stylelint .' 'prettier --check --log-level warn .'",
    "lint:fix": "concurrently -r 'eslint --fix .' 'stylelint --fix .' 'prettier --write --log-level warn .'",
    "ver": "node ./src/bin/get-version",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o .storybook-static",
    "algolia": "node --loader ./src/bin/loader.mjs --experimental-specifier-resolution=node ./src/bin/algolia.ts",
    "ut": "node --experimental-vm-modules --experimental-json-modules --experimental-specifier-resolution=node $(yarn bin jest)",
    "ut:debug": "node --experimental-vm-modules --experimental-json-modules --experimental-specifier-resolution=node --inspect $(yarn bin jest) --runInBand",
    "e2e": "cross-env NODE_ENV=test playwright test",
    "e2e:linux": "node ./src/bin/playwright.js"
  },
